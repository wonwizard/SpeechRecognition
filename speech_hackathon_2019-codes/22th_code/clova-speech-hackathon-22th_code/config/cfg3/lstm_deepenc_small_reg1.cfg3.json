{
    "config_version": 3,
    "model":{
        "enc":{
            "layer_size": 3
        },
        "dec":{
            "layer_size": 1,
            "use_attention": true,
            "max_len": 80
        },
        "rnn_cell": "lstm",
        "hidden_size": 256,
        "dropout": 0.2,
        "bidirectional": true
    },
    "data":{
        "use_mel_scale": true,
        "spec_augment": {
            "use": true,
            "time_warping_para": 160,
            "frequency_masking_para": 80,
            "time_masking_para": 150,
            "frequency_mask_num": 1,
            "time_mask_num": 1
        },
        "trim_silence": {
            "use": false
        }
    },
    "batch_size": 8,
    "workers": 2,
    "max_epochs": 100,
    "lr": 1e-04,
    "teacher_forcing": 0.3
}
